block content
    .container
        h1 Pick A Stock
            form(method='POST', action='/quote')
                label symbol
                input(type='text', name='symbol', placeholder='Enter Symbol')
                button(type='submit') Get Quote

            if results
                ul Click for more details:
                    each result, i in results
                        li
                            a(href="/detail/#{result.Symbol}") #{result.Symbol} #{result.Name}
